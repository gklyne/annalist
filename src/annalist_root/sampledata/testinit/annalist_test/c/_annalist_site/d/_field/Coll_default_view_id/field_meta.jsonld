, "annal:field_value_type":     "annal:Text"
, "annal:field_entity_type":    "annal:Collection"
, "annal:property_uri":         "annal:default_view_id"
, "annal:field_placement":      "small:0,12"
, "annal:default_value":        ""
, "annal:placeholder":          "(default view id)"
, "annal:tooltip":              "Default view identifier for showing collection content, used in conjuction with 'Default view type' and 'Default view entity'."
}
